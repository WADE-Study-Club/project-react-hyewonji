      }\n        });\n         \n        if (userPassword.length === 0){\n          alert(\"비밀번호가 잘못되었습니다.\");\n        } else {\n          alert(\"로그인되었습니다!\")\n          setIsLogin(true);\n        }\n      }\n    };\n  };\n  return (\n    <>\n      <HelmetComponent title='Login'></HelmetComponent>\n      <FormTemplate \n        onPage=\"login\"\n        onEmailChange={handleEmailChange} \n        onPasswordChange={handlePasswordChange} \n        onSubmit={handleSubmit}>\n      </FormTemplate>\n    </>\n  );\n}\n\n\nexport default Login;\n","/Users/jihyewon/Documents/weather_app/src/routes/Home.js",["76"],"import React, { useState } from 'react'\nimport HelmetComponent from '../components/HelmetComponent';\nimport HomeForm from '../components/HomeForm'\nimport NavBar from '../components/NavBar'\n\nconst Home = () => {\n\n    return (\n        <>\n            <HelmetComponent title=\"Home\" />\n            <NavBar></NavBar>\n            <HomeForm></HomeForm>\n        </>\n    )\n}\n\nexport default Home","/Users/jihyewon/Documents/weather_app/src/components/NavBar.js",["77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":7,"column":7,"nodeType":"84","messageId":"85","endLine":7,"endColumn":14},{"ruleId":"86","severity":1,"message":"87","line":8,"column":1,"nodeType":"88","endLine":18,"endColumn":3},{"ruleId":"82","severity":1,"message":"89","line":1,"column":8,"nodeType":"84","messageId":"85","endLine":1,"endColumn":13},{"ruleId":"82","severity":1,"message":"90","line":21,"column":10,"nodeType":"84","messageId":"85","endLine":21,"endColumn":17},{"ruleId":"91","severity":1,"message":"92","line":59,"column":50,"nodeType":"93","messageId":"94","endLine":59,"endColumn":52},{"ruleId":"91","severity":1,"message":"92","line":68,"column":56,"nodeType":"93","messageId":"94","endLine":68,"endColumn":58},{"ruleId":"82","severity":1,"message":"95","line":1,"column":17,"nodeType":"84","messageId":"85","endLin